{
  "name": "dennisdigital/dev_scripts",
  "description": "Install Drupal console with custom commands to be used for Drupal development.",
  "keywords": [
    "Drupal",
    "Console",
    "Development",
    "Symfony"
  ],
  "type": "project",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dennisinteractive/drupal_console_commands.git",
      "vendor-alias": "dennisdigital",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "git@github.com:dennisinteractive/drupal-console-launcher.git",
      "vendor-alias": "drupal",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "git@github.com:dennisinteractive/drupal-console-core.git",
      "vendor-alias": "drupal",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "git@github.com:dennisinteractive/drupal-console-extend.git",
      "vendor-alias": "drupal",
      "no-api": true
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-create-project-cmd": [
      "./scripts/post-create-project.sh"
    ]
  },
  "require": {
    "drupal/console-launcher": "dennis-master-dev",
    "drupal/console-extend": "dennis-master-dev",
    "dennisdigital/drupal_console_commands": "drupal_extend-dev"
  }
}
