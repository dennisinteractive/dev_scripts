{
  "name": "dennisdigital/dev_scripts",
  "description": "Install Drupal console with custom commands to be used for Drupal development.",
  "keywords": [
    "Drupal",
    "Console",
    "Development",
    "Symfony"
  ],
  "type": "project",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dennisinteractive/drupal_console_commands.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-create-project-cmd": [
      "./scripts/post-create-project.sh",
      "drupal chain --file=./vendor/dennisdigital/drupal_console_commands/scripts/post-update.yml"
    ]
  },
  "require": {
    "drupal/console": "1.0.0-beta5",
    "dennisdigital/drupal_console_commands": "0.2"
  }
}
